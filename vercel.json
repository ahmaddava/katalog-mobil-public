{
    "version": 2,
    "framework": null,
    "functions": {
        "api/index.php": {
            "runtime": "vercel-php@0.6.0"
        }
    },
    "routes": [
        {
            "src": "/(.*)",
            "dest": "/api/index.php"
        }
    ],
    "env": {
        "APP_ENV": "production",
        "APP_DEBUG": "true",
        "APP_URL": "https://katalog-mobil.vercel.app",
        "APP_KEY": "base64:...",
        "DB_CONNECTION": "pgsql",
        "DB_HOST": "...",
        "DB_PORT": "5432",
        "DB_DATABASE": "postgres",
        "DB_USERNAME": "postgres",
        "DB_PASSWORD": "..."
    },
    "outputDirectory": "public"
}